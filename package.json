{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "netlify:dev": "netlify dev",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "security-check": "npm audit && npm outdated",
    "validate-env": "node scripts/verify-env.cjs",
    "test-security": "npm run test:run && npm run security-check && npm run validate-env",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "add-admin": "node scripts/add-admin.js",
    "setup-db": "node scripts/setup-database.js",
    "check-db": "node scripts/check-database.js",
    "audit": "npm audit",
    "audit-fix": "npm audit fix",
    "security-monitor": "node scripts/security-monitor.js",
    "security-test": "node scripts/security-test.js",
    "security-scan": "npm run security-monitor && npm run audit && npm run security-test",
    "security-update": "npm audit fix && npm update",
    "codeql-setup": "node scripts/enable-codeql.js",
    "verify-env": "node scripts/verify-env.js",
    "migrate-console-logs": "node scripts/migrate-console-logs.js",
    "migrate-console-logs:dry": "node scripts/migrate-console-logs.js --dry-run",
    "clean-production-logs": "npm run build && echo 'Production build completed with console cleanup'",
    "check-file-length": "node scripts/check-file-length.js",
    "pre-commit": "node scripts/check-file-length.js --pre-commit && npm run lint",
    "setup-hooks": "chmod +x .githooks/pre-commit && git config core.hooksPath .githooks",
    "quality-check": "npm run check-file-length && npm run lint && npm run security-scan"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.0",
    "@supabase/supabase-js": "^2.49.8",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.3.4",
    "dompurify": "^3.2.6",
    "dotenv": "^16.4.5",
    "libphonenumber-js": "^1.12.8",
    "lucide-react": "^0.511.0",
    "nodemailer": "^7.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "stripe": "^18.1.1",
    "terser": "^5.40.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/stripe": "^8.0.416",
    "@vitest/ui": "^3.2.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^10.4.5",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.0",
    "typescript-eslint": "^8.18.1",
    "vitest": "^3.2.3"
  },
  "overrides": {
    "esbuild": "^0.25.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
